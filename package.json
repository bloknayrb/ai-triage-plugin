{
	"name": "ai-triage-plugin",
	"version": "0.3.0",
	"description": "Priority dashboard for Obsidian with PIP coaching, habit tracking, and weekly report generation",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"ai",
		"triage",
		"ollama",
		"tolling"
	],
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^20.19.30",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"obsidian": "latest",
		"zod": "^3.24.0"
	}
}
